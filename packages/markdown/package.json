{
  "name": "vue-markdown-next",
  "version": "0.1.2",
  "description": "Vue component to render markdown.",
  "repository": {
    "url": "https://github.com/Cat1007/vue-markdown-next"
  },
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --p tsconfig.build.json"
  },
  "module": "./lib/vue-markdown-next.mjs",
  "main": "./lib/vue-markdown-next.js",
  "exports": {
    ".": {
      "import": "./lib/vue-markdown-next.mjs",
      "require": "./lib/vue-markdown-next.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib",
    "README.md"
  ],
  "author": "Cat1007",
  "license": "MIT",
  "devDependencies": {
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vueuse/core": "^12.7.0",
    "hast-util-to-jsx-runtime": "^2.3.2",
    "html-url-attributes": "^3.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "typescript": "5.4.2",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vfile": "^6.0.3",
    "vite": "^6.0.6",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  }
}
